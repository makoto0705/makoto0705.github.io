<script>
    $(document).ready((function($){
        // top_image container
        var topImageContainer = $('#articleTopImageContainer');
                    var fixedTopImg = 1;
            if (parseInt(fixedTopImg)) {
                topImageContainer.addClass('articleTopImageContainerFixed');
            }

            $('#articleTopImageContainer img').load(function () {
                $("#articleTopImageContainerWrapper").css('height', $(this).css('height'));
            }).attr("src", 'https://likecms.s3-ap-northeast-1.amazonaws.com/top_image/1470215703_7BFrYlAWiR.jpeg');

            window.addEventListener("onorientationchange", function() { changeTopImageContainerHeight() }, false);
            window.addEventListener("resize", function() { changeTopImageContainerHeight() }, false);
            function changeTopImageContainerHeight() {
                $("#articleTopImageContainerWrapper").css('height', $('#articleTopImageContainer img').height());
            }
        
        // social button
        var snsContainer = $('#social-container');
        var snsButtonCluster = $('#snsButtonCluster');
        snsContainer.html(snsButtonCluster.html());
        // set date display
        $("div#updateTime").find("p").text(getTime());

                    // ga click event
        let adplanFlag = $('#adplanFlag').val();
            $('#articleContentContainer').find('.gaClickEvent').each(function(index, link) {
                var linkObject = $(link);
                linkObject.click(function(){
                    var gaEventLabel = linkObject.attr('data-gaevent-label');
                    if(gaEventLabel) {
                        var urlTag = "";
                        gaEventLabel = gaEventLabel + urlTag;
                    } else {
                        gaEventLabel = "anf/3RD_10_27/";
                    }
                    var href = linkObject.attr('href')? linkObject.attr('href') : "http://news.like.jp";
                    var target = linkObject.attr('target')?  linkObject.attr('target') : "_self";
                    var gaEventCategory = linkObject.attr('data-gaevent-category')? linkObject.attr('data-gaevent-category') : 'button';
                    var gaEventAction = linkObject.attr('data-gaevent-action')? linkObject.attr('data-gaevent-action') : 'click';
                    var gaEventValue = linkObject.attr('data-gaevent-value')? linkObject.attr('data-gaevent-value') : '1';
                    let gaObj = { hitCallback: function () { window.open(href , target); }};
                    if(adplanFlag==="1") {
                        gaObj = { hitCallback: function () {
                                let adpCdUrl = (window._adp && window._adp.decorate) ? window._adp.decorate(href) : href; // ADPLANのdecorate関数でhrefをラップする。
                                window.open(adpCdUrl , target);
                            }};
                    }
                    ga('send', 'event', gaEventCategory, gaEventAction, gaEventLabel, gaEventValue, gaObj);
                    return false;
                });
            });
            })(jQuery));
    function getTime(){
        var date = new Date();
        return date.getFullYear() + "年" + (date.getMonth()+1) + "月" + date.getDate() + "日";
    }
</script>

