            <script type="application/javascript">
        $('img').on('load', function(){
            $('.simpleBox.blockBox.likecmsColorPicker.alignImage').each(function() {
                var imgMaxWidth = $(this).find('p:has(img) img').map(function(){
                    return $(this).width();
                }).get()[0];
                if (imgMaxWidth > 0 && (imgMaxWidth / $(this).width() * 100) < 80) {
                    var paragraph = $(this).find('p').not(':has(img)').not(':eq(0)');
                    paragraph.each(function () {
                        $(this).css({
                            'padding-left':(imgMaxWidth + 26) + 'px'
                        })
                    });
                }
            });
        });

        jQuery(function() {
            jQuery.scrollDepth();
        });
    </script>
