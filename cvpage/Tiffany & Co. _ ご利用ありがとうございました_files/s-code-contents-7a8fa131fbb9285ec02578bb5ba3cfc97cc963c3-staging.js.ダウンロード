/* Plugin Config */
dtm_s.usePlugins = true;

function s_doPlugins(s) {

    var getVar = _satellite.getVar,
        publishDate = (_satellite.publishDate || '').split(' ')[0].replace(/-/g, '.', 'g');

    window.dataLayer = window.dataLayer || {};

    if(!s.linkType) {
        window.dtmHasPageView = true;
    }
    s.trackDownloadLinks = true;
    s.trackExternalLinks = true;

    s.server   = location.hostname.toLowerCase();
    s.pageName = getVar('page_name')||s.pageName||'';
    s.pageType = getVar('sc_pageType');
    s.channel  = getVar('sc_channel');
    s.currencyCode = getVar('ecommerce_currency');
    s.hier1    = getVar('sc_localHierarchy');
    s.hier2    = getVar('sc_globalHierarchy');
    s.prop17   = '';
    s.prop1    = getVar('page_template');
    s.eVar24   = getVar('user_checkout_status');
    s.eVar50   = getVar('user_signup_date');
    s.eVar51   = getVar('user_relationship');
    s.eVar53   = getVar('user_id');
    s.eVar59   = getVar('sc_errors');
    s.eVar64   = getVar('sc_translatedPageName');
    s.prop70   = getVar('high_resolution_screen') ? 'high resolution' : 'normal resolution';
    s.prop71   = window.location.protocol + (window.location.protocol === 'https:' ? s.trackingServerSecure : s.trackingServer);
    s.prop72   = getVar('page_server');
  	s.prop73   = getVar('datalayer_qa') + '|' + s.lastUpdated + '|' + publishDate + '|' + s.version + '|' + (typeof _satellite.getVisitorId === 'function' && _satellite.getVisitorId() !== null ? _satellite.getVisitorId().version : '');
    s.eVar83   = s.todaysDate();
    s.eVar84   = getVar('user_ecmid') || s.getQueryParam('ecmid') || '';
    s.eVar86   = getVar('search_sort');
    s.eVar91   = getVar('referrer_url');
    s.eVar94   = getVar('gclid') || s.getQueryParam('gclid') || '';

    s.previousPageName = s.getPreviousValue(s.eVar64,'s_gpv_v66');

    /* Automatic tracking should be pushed into DL format */
    if(s.linkType === 'd' && dataLayer.events) {
        dataLayer.events.download = true;
        dataLayer.download = dataLayer.download || {};
        dataLayer.download.filename = dataLayer.download.filename || s.linkURL;
    }
    if(s.linkType === 'e' && dataLayer.events) {
        dataLayer.events.exit = true;
        dataLayer.exit = dataLayer.exit || {};
        dataLayer.exit.linkURL = dataLayer.exit.linkURL || s.linkURL;
    }

    /* Set the products and event strings */
    s.events   = s.events || '';
    s.products = getVar('sc_products');

  	var events = (dataLayer.events) ? dataLayer.events : {};

    var newEvents = (getVar('sc_events') || ''),
        newVars = (getVar('sc_linkTrackVars') || {});
    if(newEvents) {
        newEvents = newEvents.split(',');
        /* This is a call from _trackData */
        if(s.linkType) {
            s.linkTrackVars = s.trackEveryCall.join(',') + ',events';
            s.linkTrackEvents = '';
        }
        /* Get all the new events */
        for(var i=0,l=newEvents.length; i<l; i++) {
            if(!newEvents[i]) { continue; }
            /* .apl doesn't dedupe serialized/events with values */
            var eventName = newEvents[i].split(/[:=]/)[0],
                eventRegex = new RegExp('(?:^|,)(' + eventName + ')(?:[,:=]|$)');
            if(!eventRegex.test(dtm_s.events)) {
                s.events = s.apl(s.events, newEvents[i], ',', 2);
            }

            /* Add in any required variables from this link track call */
            if(typeof newVars == 'object') {
                for(var newVar in newVars) {
                    if (newVars.hasOwnProperty(newVar)) {
                        s[newVar] = getVar(newVars[newVar]);
                        if (s.linkType) {
                            s.linkTrackVars = s.apl(s.linkTrackVars, newVar, ',', 2);
                        }
                    }
                }
            }

            if(s.linkType) {
                s.linkTrackEvents = s.apl(s.linkTrackEvents, eventName, ',', 2);
            }
        }
    }

    if(events.bannerImpression) {
        s.eVar5    = getVar('page_banner_type') + (getVar('page_banner_code') ? '|' + getVar('page_banner_code') : '');
    }

    if(events.share) {
        s.eVar46 = getVar('share_type');
        s.linkTrackVars += ',eVar46';
    } else {
        s.eVar46 = '';
    }

    if(events.navClick) {
        s.eVar8 = getVar('nav_location') + '|' + (getVar('nav_link')||[]).join(',');
        s.prop3 = 'D=v8';
        s.linkTrackVars += ',eVar8,prop3';
    }

    if((events.productView || events.addToCart || events.addSavedItem) && dataLayer.products && dataLayer.products[0] && dataLayer.products[0].position) {
        s.eVar26 = dataLayer.products[0].position;
        s.eVar26 += '|' + (dataLayer.products[0].isNew === true ? 'new' : 'standard');
        s.eVar26 += '|' + (dataLayer.products[0].isHighlight === true ? 'highlight' : 'standard');
        s.linkTrackVars += ',eVar26';
    } else {
        s.eVar26 = '';
    }

    if(events.productView || events.statementView) {
        s.eVar74 = getVar('sc_stockMessage');
    } else {
        s.eVar74 = '';
    }

    //If there is a finding method - put it in eVar71 and append the previous page name
  if( getVar('products') && getVar('products')[0] && getVar('products')[0].findingMethod ) {
        var pvpn = s.previousPageName == 'no value' ? '' : s.previousPageName;
        var pfm  = getVar('products')[0].findingMethod;
        var s_eVar71 = [pfm, pvpn].join('##').replace(/##$/,'');
        s.eVar71 = s_eVar71;
        s.linkTrackVars = s.apl(s.linkTrackVars,'eVar71',',',2);
        console.log('zed:pfm', s.eVar71);
    }


    if(events.viewProductTab && getVar('interaction_type') === 'tab') {
        s.eVar48 = getVar('interaction_name');
        s.prop43 = 'D=v48';
        _satellite.setVar('_newProducts', true);
        s.linkTrackVars += ',prop43,eVar48';
    } else {
        s.eVar48 = s.prop43 = '';
    }

    if(events.productImageInteraction && getVar('interaction_type') === 'product image') {
        s.eVar36 = getVar('interaction_name') + '|' + getVar('interaction_file');
        _satellite.setVar('_newProducts', true);
        s.linkTrackVars += ',eVar36';
    } else {
        s.eVar36 = '';
    }

    if(events.siteInteraction) {
        s.eVar55 = (getVar('interaction_type') + ' - ' + getVar('interaction_name')).toLowerCase();
        s.prop55 = 'D=v55';
        if(getVar('interaction_type') === 'pdp') {
            _satellite.setVar('_newProducts', true);
        }

        s.linkTrackVars += ',prop55,eVar55';
    } else {
        s.prop55 = s.eVar55 = '';
    }

    if(events.faqSearch) {
        s.eVar27 = getVar('search_faq_keyword');
        s.linkTrackVars += ',eVar27';
    } else {
        s.eVar27 = '';
    }



			if( (getVar('search_sortTool') || getVar('search_sort')) && !events.filterAdded && !events.filterRemoved) {
			   //set v86, v88
        s.eVar86 = (getVar('search_sortTool') + ":" + getVar('search_sort')).replace(/(^:|:$)/g,'');
			   s.eVar88 = "D=v86";
			   s.linkTrackVars += ",eVar86,eVar88";
			}
			if( events.sortUpdate || events.filterAdded ) {
         s.prop41 = s.eVar87 = '';
			   if( !events.pageView && events.sortUpdate)  s.prop41 = "view by:" + getVar('search_sort');
			   if( !events.pageView && events.filterAdded) s.prop41 = getVar('search_filter_last');
			   s.linkTrackVars += ",prop41";

			   if(events.filterAdded) {
				  var lastFilter = getVar('search_filter_last_raw');
				  if(typeof lastFilter == 'object') {
					  if(typeof lastFilter.type !== 'string' || lastFilter.type.toLowerCase() !== 'gift finder') {
						  s.eVar87 = getVar('search_filter_all');
						  s.linkTrackVars += ",eVar87";
					  } else if(typeof lastFilter.type === 'string' || lastFilter.type.toLowerCase() !== 'gift finder') {
						  //s.eVar88 = getVar('search_filter_last');
					  }
				  }
			   }
			}
      //Do not fire filter/sort variables on personalize overlay or if load more results is clicked in the mobile grid
      if(events.loadMoreResults || events.personalize) {
          s.prop41 = s.eVar86 = s.eVar87 = s.eVar88 = '';
      }


    if(events.filterRemoved) {
        // Do not fire a call to Adobe on removal
        s.abort = true;
    }

    if(events.storeLocatorSearch || events.findInStoreSearch) {
        s.eVar49 = getVar('store_locator_keyword');
        s.prop37 = 'D=v49';
        s.linkTrackVars += ',eVar49,prop37';
    } else {
        s.eVar49 = '';
    }

    if(events.viewStoreEvent || events.selectStore || (events.siteInteraction && (getVar('interaction_type') === 'store events' || getVar('interaction_type') === 'store locator'))) {
        s.eVar75 = getVar('store_locator_store_ID');
        if(s.eVar75) { s.prop16 = 'D=v75'; }
        s.eVar76 = getVar('store_locator_event_ID');
        s.linkTrackVars += ',prop16,eVar75,eVar76';
    } else {
        s.eVar76 = s.prop16 = s.eVar75 = '';
    }
    if(events.shippingInformation) {
        // moved to checkoutclick - 180427
        // s.eVar45 = getVar('products_total_quantity');
    }
    if(events.addToCart) {
      // moved to purchase event only - 180515
      // s.eVar30 = getVar('products_engraving_all');
      // s.linkTrackVars += ',eVar30';
    }
    if(events.checkoutClick) {
       s.eVar45 = getVar('products_total_quantity');
       s.linkTrackVars += ',eVar45';
    }

    if(events.purchase) {
        s.purchaseID   = getVar('ecommerce_purchaseID');
        s.eVar6    = getVar('ecommerce_payment_method');
        s.eVar54   = s.eVar54 || getVar('user_order_count');
        s.eVar29   = getVar('ecommerce_shipping_method');
        s.eVar30   = getVar('products_engraving_all');
        s.eVar12 = s.transactionID = 'D=purchaseID';
        s.eVar75 = getVar('ecommerce_storeID');
    }

    if(events.videoStart || events.videoMilestone || events.videoComplete) {
        var videoName = [];
        if(getVar('video_name')) {
            videoName.push(getVar('video_name'));
        }
        if(getVar('video_id')) {
            videoName.push(getVar('video_id'));
        }
        s.eVar37 = videoName.join(' - ');
        s.linkTrackVars += ',eVar37';
    } else {
        s.eVar37 = '';
    }

    /* Temp BB16 fix */
    if(events.statementView) {
        s.prop1 = 'Statement Product Detail Page';
        s.events = s.events.replace(/((event79|prodView),?)/gi, '').replace(/,$/, '');
    }
    if(window.location.pathname.toLowerCase() == '/explore/blue-book-2016/statement') {
        s.prop1 = 'Blue Book Statement';
        s.hier1 = s.hier1.replace('explore,blue-book-', 'shopping,blue book ');
        s.hier2 = s.hier2.replace('explore,blue-book-', 'shopping,blue book ');
    }

    /* Temp PLP fix */
    if((getVar("page_template")||"").toLowerCase() === "product landing page" && getVar("search_categoryID") && getVar("search_category_name")) {
        s.prop27 = s.eVar95 = getVar("search_categoryID") + ":" + getVar("search_category_name");
        s.linkTrackVars += ',prop27,eVar95';
    }

    /* Add calls to plugins here */
    if(s.getQueryParam('omcid')) {
        s.campaign = s.getQueryParam('omcid');
        s.campaign = s.cleanParams(s.campaign);
        if(s.campaign == 'cyc') {
            s.campaign = "";
        }
    }
    if(!s.campaign && s.getQueryParam('utm_source') && s.getQueryParam('utm_medium') && s.getQueryParam('utm_campaign')) {
        s.campaign = s.getQueryParam('utm_source') + "|" + s.getQueryParam('utm_medium') + "|" + s.getQueryParam('utm_campaign');
    }
    if(!s.campaign && s.getQueryParam('gclid')) {
        s.campaign = "ppc google autotagging";
    }
    if(!s.campaign && s.getQueryParam('mkwid')) {
        s.campaign = "ppc marin autotagging";
    }


    if(s.campaign) {
        s.campaign = decodeURIComponent(s.campaign);
        s.prop6 = s.campaign + " : " + s.pageName;
        s.eVar42 = s.campaign + " : " + s.pageName;
    } else {
        //s.prop6=s.pageName;
    }

    //Internal Campaigns
    if(!s.eVar3 && s.getQueryParam('hppromo')) {
        s.eVar3 = s.getQueryParam('hppromo');
        s.eVar3 = s.cleanParams(s.eVar3);
        var s_eVar3 = s.eVar3
    }

    if(!s.eVar28 && s.getQueryParam('lppromo')) {
        s.eVar28 = s.getQueryParam('lppromo');
        s.eVar28 = s.cleanParams(s.eVar28);
        var s_eVar28 = s.eVar28
    }

    if(s.getQueryParam('cid')) {
        s.eVar4 = s.getQueryParam('cid');
        s.eVar4 = s.cleanParams(s.eVar4);
        var s_eVar4 = s.eVar4
    }
    var internalCampaign = s.eVar3 || s.eVar28;
    dtm_s.prevIntCamp = dtm_s.prevIntCamp || '';

    if(internalCampaign !== dtm_s.prevIntCamp) {
        s.eVar21 = s.crossVisitParticipation(internalCampaign, 's_v21_cvpn', '7', '10', '>', 'purchase', 2);
        s.linkTrackVars=s.apl(s.linkTrackVars,'eVar21',',',2);
    } else {
        s.eVar21 = '';
    }
    dtm_s.prevIntCamp = internalCampaign;

    s.eVar11 = s.prop12 = s.getTimeParting('n', '-5');
    if(s.pageType == 'errorPage') {
        s.prop22 = getVar('page_errors_url') || getVar('page_url');
    }
    if(s.eVar2) {
        s.eVar2 = s.eVar2.toLowerCase();
        s.prop18 = 'D=v2';
    }
    if(s.campaign) {
        s.eVar32 = s.campaign;
    }
    s.prop23 = 'D=g';
    s.prop24 = s.eVar90 = 'D=v64';
    s.prop37 = 'D=v49';
    s.eVar41 = 'D=h2';
    s.eVar44 = 'D=cc';
    s.prop44 = 'D=v52';
    s.prop45 = (s.eVar53) ? 'D=v53' : '';


    s.eVar65 = window.location.hostname + window.location.pathname;
    s.prop75 = 'D=v65';
    s.eVar85 = window.location.search;
    s.eVar89 = 'D=c1';

    if(s.products != '') { s.eVar70 = 'D=products'; }

    if(events.exit) {
        s.eVar57 = (getVar('exit_url') || '').replace(/(f|ht)tps?:\/\//,'');
        s.linkTrackVars += ',eVar57';
    } else {
        s.eVar57 = '';
    }

    if(events.download) {
        s.eVar58 = (getVar('download_filename') || '').replace(/(f|ht)tps?:\/\//,'');
        s.linkTrackVars += ',eVar58';
    } else {
        s.eVar58 = '';
    }

    /* every call */
    if (s.linkType) {
        for (var n=0,v=s.trackEveryCall.length;n<v;n++) {
            s.linkTrackVars=s.apl(s.linkTrackVars,s.trackEveryCall[n],',',2);
        }
        if(getVar('_newProducts') || events.share) {
            s.linkTrackVars=s.apl(s.linkTrackVars,'products',',',2);
        }
        /*
        if(s.eVar8) {
            s.events = s.apl(s.events, 'event56', ',', 2);
            s.linkTrackEvents=s.apl(s.linkTrackEvents,'event56',',',2);
        }
        */
        s.eVar66 = s.eVar68 = '';
        s.eVar33 = s.events + '|' + s.linkTrackEvents;

    } else {
        s.eVar66 = s.previousPageName == 'no value' ? '' : s.previousPageName;
        s.eVar77 = s.getPreviousValue(s.prop1,'s_gpv_v77');
        s.eVar68 = '+1';

        s.eVar56 = s.crossVisitParticipation(s.campaign, 's_v56_cvp', '365', '10', '>', 'purchase', 2);
        s.eVar33 = s.events;

        //Grid Finding Method and Previous Page Template concatenated.
        s.eVar78 = getVar('search_finding_method');
        if(s.eVar78) {
            if(s.eVar77) s.eVar78 += "|" + s.eVar77;
            s.prop42 = "D=v78";
        }
    }


    if(s.inList('event41', s.events, ',')) {
        s.eVar69 = '+1';
        s.linkTrackVars=s.apl(s.linkTrackVars,'eVar69',',',2);
    } else {
        s.eVar69 = '';
    }

    // Time to Purchase
    // *Add To Cart to Purchase - Rounded down
    s.eVar81 = s.getTimeBetweenEvents("scAdd","purchase","s_gtbe_add_pur",30,"d");
    // *First Visit to Purchase - Rounded down
    s.eVar82 = s.getTimeBetweenEvents(false,"purchase","s_gtbe_vis_pur",90,"d",true);
    // *Add To Cart to Purchase - Exact
    var addPurEvent = s.getTimeBetweenEvents("scAdd","purchase","s_gtbe_add_pur_ev",30,false);
    // *First Visit to Purchase - Exact
    var visPurEvent = s.getTimeBetweenEvents(false,"purchase","s_gtbe_vis_pur_ev",90,false,true);

    if (addPurEvent.indexOf("error") == -1 && addPurEvent != ""){
        s.events = s.apl(s.events, 'event87='+s.timeToNumeric(addPurEvent,"m"), ',', 2);
    }
    if (visPurEvent.indexOf("error") == -1 && visPurEvent != ""){
        var visitToPurchaseTime = visPurEvent.split("|");
        s.events = s.apl(s.events, 'event88='+s.timeToNumeric(visitToPurchaseTime[0],"d"), ',', 2);
    }

    s.plugins = 'x';


    // move events to QA element
    window.dataLayer.rawEvents = window.dataLayer.events;
    // clear out data layer events (temp)
    window.dataLayer.events = {};

}
dtm_s.doPlugins = s_doPlugins;
dtm_s.wd = dtm_s.w;
/************************** PLUGINS SECTION *************************/
/* You may insert any plugins you wish to use here.                 */


/**
  * Plugin: todaysDate
  * @return string YYYY-MM-DD
*/
dtm_s.todaysDate = function() {
    var _today = new Date(),
        _dd    = _today.getDate(),
        _mm    = _today.getMonth()+1,
        _yyyy  = _today.getFullYear();
    if(_dd<10) _dd='0'+_dd;
    if(_mm<10) _mm='0'+_mm;
    return _yyyy+'-'+_mm+'-'+_dd;
}
/**
 * Plugin: timeToNumeric
 * @param string [dd:hh:mm]
 * @param char
    * "d" to return time converted to days
    * "m" to return time converted to minutes
 * @return string number
**/
dtm_s.timeToNumeric = function(t,f){
    if (typeof t == "string" && /[0-9]+:[0-9]+:[0-9]+/.test(t)){
        var time = t.split(":");
        if (f == "d") {
            return parseFloat(parseInt(time[0])+parseInt(time[1])/24+parseInt(time[2])/(24*60)).toFixed(2);
        } else {
            return parseInt(time[0])*24*60+parseInt(time[1])*60+parseInt(time[2]);
        }
    } else return "0.00";
}
/**
 * Plugin: lPS (leftPadString)
**/
dtm_s.lPS = new Function("v","w","z",""
+"z=String(z||'0');v=v+'';return v.length>=w?v:new Array(w-v.length+1"
+").join(z)+v;");
// end leftPadString

/**
 * Plugin: getTimeDifference
**/
dtm_s.getTimeDifference= new Function("b","e","r","p",""
+"var s=this;var p=Number(p||0);var r=r||'';if(r!='u'||r!=='d')r==fal"
+"se;var t=new Date();t.setTime(e-b);td=t.getTime();var d=Math.floor("
+"td/86400000);td-=d*86400000;var h=Math.floor(td/3600000);td-=h*3600"
+"000;var m=Math.ceil(td/60000);if(r){if(h>0){if(m>0){if(r=='u')h++;m"
+"=0;}}if(d>0){if((h>0)||(m>0)){if(r=='u')d++;h=0;m=0;}}}d=s.lPS(d,p,"
+"0);h=s.lPS(h,p,0);m=s.lPS(m,p,0);return d+':'+h+':'+m;");
// end getTimeDifference

/**
  * Plugin: getTimeBetweenEvents
**/
dtm_s.getTimeBetweenEvents=new Function("e1","e2","c","d","rc","ro",""
+"var s=this,rc=(rc||''),r='repeat',x='',ev=(s.events||''),c=(c||'s_g"
+"tbe'),ro=(ro||false),d=(Number(d)||false);var o=s.c_r(c).split('|')"
+";var t=(new Date()).getTime();if(d){x=new Date;x.setTime(x.getTime("
+")+1000*60*60*24*d);}if(!e2)return '';if(!e1||s.inList(e1,ev,',',':'"
+")){if(!o[0]){s.c_w(c,t,x);}}if(s.inList(e2,ev,',',':')){if(o[0]){if"
+"(o[1]==r)return s.getTimeDifference(o[0],t,rc,2)+'|'+r;if(ro)s.c_w(c,o[0]+'|'+r,x);else s.c_w(c,'',-100)"
+";return s.getTimeDifference(o,t,rc,2);}else{return 'error: e2 before "
+"e1';}}return ''");
/* Fix AM's Util case-insensitivity */
dtm_s.getQueryParam = dtm_s.Util.getQueryParam = function(n,u,d) {
    try {
        if(!n) { return; }
        var d = d || '&',
            u = u || window.location.search,
            x = new RegExp('[\?'+d+']'+n+'=([^'+d+']+)' , 'i'),
            r = u.match(x);
        return (r && r[1]) ? r[1] : '';
    } catch(e) { }
};

