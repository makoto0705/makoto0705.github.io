return true;for(var i=0;i<tagArray.length;i++){if(tagArray[i]==tag){return true;}}return false;}$(document).ready(function(){$("body").on("click",".article-share a",function(e){e.preventDefault();var title='Tiffany & Co. - '+$("h1").text();var text=$("article p:first").text();var image=window.location.origin+$("figure img").attr("src");var shortImage=$("figure img").attr("src");var url=window.location.origin+window.location.pathname;var shortUrl=window.location.pathname;if($(this).hasClass("facebook")){postToFaceBook(title,shortUrl,'New and Noteworthy','SMPF1&utm_campaign=Social_Media_Shared_Link&utm_medium=social_media&utm_source=Facebook_Share&utm_content=&utm_term=',text,shortImage);}if($(this).hasClass("tumblr")){postToTumblr(title,url,'image','SMPF2&utm_campaign=Social_Media_Shared_Link&utm_medium=social_media&utm_source=Tumbler_Share&utm_content=&utm_term=',text,image);}if($(this).hasClass("twitter")){postToTwitter(title,shortUrl,'New and Noteworthy','SMPF2&utm_campaign=Social_Media_Shared_Link&utm_medium=social_media&utm_source=Twitter_Share&utm_content=&utm_term=');}if($(this).hasClass("pinterest")){var description=title+": "+$.trim(text);postToPinterest(description,url,'New and Noteworthy','SMPF2&utm_campaign=Social_Media_Shared_Link&utm_medium=social_media&utm_source=Pinterest_Share&utm_content=&utm_term=',image);}if($(this).hasClass("google")){postToGoogle(url,'SMPF2&utm_campaign=Social_Media_Shared_Link&utm_medium=social_media&utm_source=Pinterest_Share&utm_content=&utm_term=');}if($(this).hasClass("weibo")){var description=title+": "+$.trim(text);postToWeibo(description,url,'New and Noteworthy','SMPF2&utm_campaign=Social_Media_Shared_Link&utm_medium=social_media&utm_source=Weibo_Share&utm_content=&utm_term=',image);}if($(this).hasClass("email")){OmnitureTrackSocialSharing("Email");if(!isMobileSite){linkEmailThisPage('EmailRequest.aspx?dest='+encodeURIComponent(window.location.pathname+"?"+window.location.search.split("?").join(""))+'&hdrOnOff=On&msgOnOf