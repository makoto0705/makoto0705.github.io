zy-loaded):visible").each(function(){var video=$(this);var videoOrig=video[0];if(video.offset().top<=loadPoint&&video.attr("preload")&&video.attr("preload")=="none"){video.addClass("lazy-loaded");video.attr("poster",video.attr("data-poster"));videoOrig.load();videoOrig.play();}});}var parallaxBoxes=[];function ParallaxBox(options,fg1Options,fg2Options){this.init(options,fg1Options,fg2Options);}function initParallaxBoxes(){$(".apply-parallax-box").each(function(){var $el=$(this);var fg2Options,fg1Options;var isBGHorizontal=false;var bgDirection=1;switch($el.attr("data-bg-direction")){case"down":bgDirection=-1;$el.addClass("parallax-box--bg-down");$el.find(".parallax-box__bg-image").css("bottom",$el.attr("data-bg-speed")*10);$el.find(".parallax-box__bg-image").addClass("parallax-box__bg-full-width");break;case"left":$el.addClass("parallax-box--bg-down");$el.find(".parallax-box__bg-image").css("left",0);$el.find(".parallax-box__bg-image").addClass("parallax-box__bg-full-height");isBGHorizontal=true;break;case"right":bgDirection=-1;isBGHorizontal=true;$el.addClass("parallax-box--bg-down");$el.find(".parallax-box__bg-image").css("right",0);$el.find(".parallax-box__bg-image").addClass("parallax-box__bg-full-height");break;default:$el.find(".parallax-box__bg-image").addClass("parallax-box__bg-full-width");}var bgOptions={bgSpeed:$el.attr("data-bg-speed"),bgDirection:bgDirection,bgHorizontal:isBGHorizontal
