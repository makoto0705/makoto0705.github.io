;}}if(isNaN(modelState.currentPage)==false&&modelState.currentPage>=0){this.initPageNum(modelState.currentPage);}if(modelState.category!=null){this.pBrowseState.category=modelState.category;}if(modelState.refinement!=null){this.pBrowseState.refinement=modelState.refinement;}if(modelState.sortCriteria!=null){this.pBrowseState.sortCriteria=Number(modelState.sortCriteria);}if(modelState.pageSize!=null){this.pBrowseState.pageSize=modelState.pageSize;}if(modelState.searchTerms!=null){this.pBrowseState.searchTerms=modelState.searchTerms;}if(modelState.relatedItemSku!=null){this.pBrowseState.relatedItemSku=modelState.relatedItemSku;}if(modelState.onlyItems!=null){this.pBrowseState.onlyItems=modelState.onlyItems;}if(modelState.isNormalized!=null){this.pBrowseState.isNormalized=modelState.isNormalized;}if(modelState.lowRange!=null){this.pBrowseState.lowRange=modelState.lowRange;}if(modelState.highRange!=null){this.pBrowseState.highRange=modelState.highRange;}if(modelState.popup!=null){if(this.pBrowseState.popup!=modelState.popup){this.pBrowseState.popup=modelState.popup;if(modelState.popup!=""){var splitPopupState=modelState.popup.split("+");if(splitPopupState.length==1){if(splitPopupState[0].substr(0,5)=="video"){displayVideoOverlay($("body").width(),splitPopupState,"/Video/VideoOverlay.aspx?mode=new&config=/shared/videos/config/"+splitPopupState[0].split("video").join("").replace("_","/")+".txt",'videoOverlay');}else{openMarketingPopUpFollowup(splitPopupState[0]);}}else if(splitPopupState.length>=2){for(var i=1;i<splitPopupState.length;i++){splitPopupState[i]=URLFactory.hashUnescape(splitPopupState[i]);}if(splitPopupState[0].substr(0,5)=="video"){displayVideoOverlay($("body").width(),splitPopupState,"/Video/VideoOverlay.aspx?mode=new&config=/shared/videos/config/"+splitPopupState[0].split("video").join("").replace("_","/")+".txt",'videoOverlay');}else{openMarketingPopUpFollowup(splitPopupState[0],splitPopupState);}}}else{closePopUpFollowup();}}}else{closePopUpFollowup();}i